@model IEnumerable<ErrorLoggerDataBase.userDetails>

@{
    ViewBag.Title = "assignToUser";
}
<title>
    Assign User
</title>
<title>Assign User</title>
@if (ViewBag.Message != null)

{ <br /><br /><div><b>@ViewBag.Message</b></div>}
else
{


<h2>Assign User</h2>
<h5 align="right">
    @Html.ActionLink("Go Back", "activeApps")
</h5>

<table class="table">
    <tr>
        <th>
          <b>User ID</b>
        </th>
        <th>
          <b>First Name</b>
        </th>
        <th>
           <b>Last Name</b>
        </th>
        
        <th>
           <b>Last Logged Time</b>
        </th>
       
        
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userDetailsID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.lastLoggedTime)
        </td>
       
        <td>
            @Html.ActionLink("Assign", "assignedUser", new { userID = item.userDetailsID }) 
          
        </td>
    </tr>
}

</table>
}