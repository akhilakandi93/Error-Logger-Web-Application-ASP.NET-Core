@model IEnumerable<ErrorLoggerDataBase.userDetails>

@{
    ViewBag.Title = "unassignUsers";
}
<title>Unassign Users</title>
<h2>Unassign Users</h2>

<h5 align="right">
    @Html.ActionLink("Go Back", "activeApps")
</h5>

<table class="table">
    <tr>
        <th>
           <b>User ID</b>
        </th>
        <th>
           <b>First Name</b>
        </th>
        <th>
            <b>Last Name</b>
        </th>
       <th>
           <b>Last Logged Time</b>
       </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userDetailsID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.lastLoggedTime)
        </td>
       
        <td>
            @Html.ActionLink("Unassign", "unassignedUser", new { userID = item.userDetailsID }) 
           
        </td>
    </tr>
}

</table>
